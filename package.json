{
  "name": "blogger-pwa",
  "version": "0.0.1",
  "private": true,
  "description": "Build a PWA for your Blogger Site",
  "license": "MIT",
  "author": {
    "name": "Deo Kumar",
    "url": "https://github.com/kumardeo"
  },
  "homepage": "https://github.com/kumardeo/blogger-pwa",
  "repository": {
    "type": "git",
    "url": "https://github.com/kumardeo/blogger-pwa"
  },
  "bugs": {
    "url": "https://github.com/kumardeo/blogger-pwa/issues"
  },
  "packageManager": "npm@11.4.2",
  "type": "module",
  "scripts": {
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:types": "tsc --noEmit",
    "generate": "tsx ./scripts/generate.ts",
    "deploy": "npm run generate && wrangler deploy",
    "dev": "npm run generate && wrangler dev",
    "upgrade": "ncu --interactive"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/node": "^24.0.7",
    "arg": "^5.0.2",
    "console-log-colors": "^0.5.0",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.25.5",
    "favicons": "^7.2.0",
    "npm-check-updates": "^18.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0",
    "wrangler": "^4.22.0",
    "xml-js": "^1.6.11"
  }
